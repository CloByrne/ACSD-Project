"paths": {
	"/contacts": {
		"get": {
			"description": "Returns an object of contacts.",
			"responses": {
				"200": {
					"description": "Returns an object of contacts."
				}
			}
		},
		"post": {
			"description": "Adds a new contact to the database. Required fields in the body of the request: {name: \"John Doe\", email: \"johndoe@example.com\", phone: \"123-456-7890\"}",
			"responses": {
				"201": {
					"description": "Returns a blank object."
				}
			}
		},
		"delete": {
			"description": "Deletes a contact from the database. Required fields in the body of the request: {contact_id: 1}",
			"responses": {
				"204": {
					"description": "Returns a blank object."
				}
			}
		},
		"put": {
			"description": "Updates a contact in the database. Required fields in the body of the request: {contact_id: 1, name: \"John Doe\", email: \"johndoe@example.com\", phone: \"123-456-7890\"}",
			"responses": {
				"204": {
					"description": "Returns a blank object."
				}
			}
		}
	}
},

"components": {
	"schemas": {
		"Contact": {
			"type": "object",
			"required": [
				"name",
				"email",
				"phone"
			],
			"properties": {
				"id": {
					"type": "integer"
				},
				"name": {
					"type": "string"
				},
				"email": {
					"type": "string"
				},
				"phone": {
					"type": "string"
				}
			}
		}
	}
}

"tags": [
	{
		"name": "Contacts"
	}
]
